    <a href="javascript:;" onclick="(function () {
        var allText = '';
        var links = document.getElementsByTagName('a');
        for(var i = 3; i < links.length; i++) {
            allText += $.ajax({ type: 'GET', url: links[i].href, async: false }).responseText;
        };
        alert('done');
        window.open('data:text/json,' + encodeURIComponent(allText));
    })();">Test iteration</a> <!-- MUST REMOVE target ATTRIBUTE -->
